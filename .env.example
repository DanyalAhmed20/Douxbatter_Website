# MySQL Database (Hostinger)
MYSQL_HOST=your_hostinger_host
MYSQL_USER=your_db_user
MYSQL_PASSWORD=your_db_password
MYSQL_DATABASE=your_db_name

# Admin Authentication
ADMIN_PASSWORD=your_secure_admin_password

# Ziina Payment Gateway
# Get your access token from: https://ziina.com/business/connect
ZIINA_ACCESS_TOKEN=your_ziina_access_token

# Webhook secret for verifying payment webhooks (optional but recommended)
ZIINA_WEBHOOK_SECRET=your_ziina_webhook_secret

# Your production domain URL (required for payment callbacks)
NEXT_PUBLIC_BASE_URL=https://your-production-domain.com
